<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="me.myfilemanager">

    <!-- To access Google+ APIs: -->

      <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>


      <application
          tools:replace="label"
          android:allowBackup="true"
          android:icon="@mipmap/ic_launcher"
          android:label="@string/myapp_name"
          android:theme="@style/myTheme">
          <activity
              android:name="me.myfilemanager.Activity.MainActivity"
              android:label="@string/myapp_name">
              <intent-filter>
                  <action android:name="android.intent.action.MAIN" />
                  <category android:name="android.intent.category.LAUNCHER" />
              </intent-filter>
          </activity>
          <activity
              android:name="me.myfilemanager.Activity.SettingActivity"
              android:label="@string/title_activity_settings"
              android:parentActivityName="me.myfilemanager.Activity.MainActivity"
>
              <meta-data
                  android:name="android.support.PARENT_ACTIVITY"
                  android:value="me.myfilemanager.Activity.MainActivity" />
          </activity>
      </application>

  </manifest>
